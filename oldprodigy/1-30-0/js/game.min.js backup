				var s = this.game.prodigy.player.state.get("world-bounties");
				s[t[a].ID - 84] = -1 * s[t[a].ID - 84], this.game.prodigy.player.state.set("world-bounties", s), this.destroyPage(t[a].ID - 84), e.prodigy.player.backpack.consume("item", t[a].ID, 1)
