	var t = Util.getDateSeed(),
		i = (new Date).getDay();
	e.quests.getZoneVar(Tech.DATA.ID, "day") !== i && (e.quests.clearQuestLine(Tech.DATA.ID), e.quests.setZoneVar(Tech.DATA.ID, "day", i));
