		this.mode = e, this.member = t, this.xpBar, this.level, this.drawCard(this.mode)
	},
	drawCard: function (e) {
		"player" === e ? this.createPlayer() : "monster" === e ? this.createMonster() : this.createEmpty(), this.stars > 0 && this.getStars()
	},
	createPlayer: function () {
