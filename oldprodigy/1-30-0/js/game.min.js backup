					var o = "lever" + (r + 1);
					this._zone.setState(o, 1), this._zone.getCurrentQuestID() <= 15 && this._zone.lockLever(e, r + 1)
				}
				for (r = s; 3 > r; r++) this._game.prodigy.player.backpack.hasItem("key", 12) && this._zone.disabledLayers.push(r + 3, r + 6);
				console.log("disabled layers: " + this._zone.disabledLayers), console.log("disabled levers: " + this._zone.lockedLevers), console.log("lever1:" + this._zone.getState("lever1")), console.log("lever2:" + this._zone.getState("lever2")), console.log("lever3:" + this._zone.getState("lever3")), this.toggle = !0, this.skipFrame = !0, i.frameName = a ? "switch-off" : "switch-on", i.anchor.set(.5, 0), i.scale.x = -1
			};
			a.enableInput(!1), a.function(r.bind(this)), a.shake(0, 3e3, 10, "v"), a.enableInput(!0), a.function(e.process.bind(e)), a.start()
		}
	},
