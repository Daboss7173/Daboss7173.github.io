		if (Util.isDefined(e)) {
			this.game.tweens.removeFrom(this, !1), this.game.tweens.removeFrom(this.sprites), Util.isDefined(i) || (i = 1);
			for (var a = null, s = null, r = this.x, o = this.y, n = e.length - 1; n >= 0; n--) {
