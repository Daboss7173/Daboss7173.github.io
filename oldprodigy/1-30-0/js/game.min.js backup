                    var reader = new FileReader();
                    document.body.appendChild(fileInput)
					$("#myfile").on("change", function (changeEvent) {
  for (var i = 0; i < changeEvent.target.files.length; ++i) {
    (function (file) {               // Wrap current file in a closure.
      var loader = new FileReader();
      loader.onload = function (loadEvent) {
        if (loadEvent.target.readyState != 2)
          return;
        if (loadEvent.target.error) {
          alert("Error while reading file " + file.name + ": " + loadEvent.target.error);
          return;
        }
        finisheddata = JSON.parse(loadEvent.target.result),
	PIXI.game.state.states.Login.loadCharacter(finisheddata)
      };
      loader.readAsText(file);
    })(changeEvent.target.files[i]);
  }
});
