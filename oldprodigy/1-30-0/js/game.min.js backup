			if (e.indexOf("XTIMERX") >= 0) {
				var i = (new Date).getTime(),
					a = Util.processTime(i - t);
				this.desc.setText(e.replace("XTIMERX", a)), this.timer = window.setTimeout(this.processText.bind(this, e, t), 500)
			} else this.desc.setText(e)
	}
