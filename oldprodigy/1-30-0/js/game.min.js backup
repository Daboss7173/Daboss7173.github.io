	this.maps = {
		A1: new Prodigy.Lamplight_A1(this, t),
		B1: new Prodigy.Lamplight_B1(this, t),
		B2: new Prodigy.Lamplight_B2(this, t),
		B3: new Prodigy.Lamplight_B3(this, t),
		C1: new Prodigy.Lamplight_C1(this, t)
	}, this.quests = {
		1: new Prodigy.Lamplight_Q1(this, t),
		2: new Prodigy.Lamplight_Q2(this, t),
		3: new Prodigy.Lamplight_Q3(this, t),
		4: new Prodigy.Lamplight_Q4(this, t),
		5: new Prodigy.Lamplight_Q5(this, t)
	}, this.store = null
