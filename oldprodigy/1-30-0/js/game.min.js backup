		for (var e = 0; e < this.items.children.length; e++) {
			var t = this.items.children[e];
			t.data.x = t.x, t.data.y = t.y, t.data.z = t.z
		}
		this.game.prodigy.player.house.updated = !0, Prodigy.Control.Menu.prototype.close.call(this)
	}
