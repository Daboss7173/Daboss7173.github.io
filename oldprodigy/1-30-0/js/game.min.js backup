					var s = e.prodigy.event.create();
					this.caught || (this.caught = !0, this.foodPlaced || (s.enableInput(!1), this.cebollini.x > t.user.x ? s.validPath(t.user, t.path, t.user.x + 1, t.user.y) : s.validPath(t.user, t.path, t.user.x - 1, t.user.y), s.enableInput(!0), s.text(0, "cebollini"), s.function(e.prodigy.world.teleport.bind(e.prodigy.world, "bonfire_spire-E6")))), s.start()
