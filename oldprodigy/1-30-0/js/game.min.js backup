		"ring" === e || Util.isDefined(this.player.equipment.getEquipment(e)) ? this.itemSelected(this.player.equipment.getEquipment(e), e, "equip") : this.game.prodigy.open.pouch(e, this.updatePlayer.bind(this))
	}
