		this.api.login(e, t, {
			401: this.processLogin.bind(this, !1, a, o),
			206: this.processLogin.bind(this, !0, s, o),
			200: this.processLogin.bind(this, !0, s, o),
			503: this.processLogin.bind(this, !1, a, o),
			340: this.processLogin.bind(this, !1, a, o),
