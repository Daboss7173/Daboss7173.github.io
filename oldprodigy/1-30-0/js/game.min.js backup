	},
	startDessert: function () {
		this.foodMode = "cake", this.npc.dessertStart();
		var e = this.game.prodigy.event.create();
		e.text(12, "cebollini"), e.enableInput(!1);
		for (var t in this.burgerParts) e.animation(this.game.prodigy.animation.hop.bind(this.game.prodigy.animation, this.burgerParts[t], "400", "-150", "+150", 2e3, !1, !0));
