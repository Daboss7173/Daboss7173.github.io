	}, ["lamplight-B1"], [], [], [], [], !0)
}, Prodigy.extends(Prodigy.Lamplight_Q1, Prodigy.Quest, {
	constructor: Prodigy.Lamplight_Q1,
	start: function (e, t, i) {
		Prodigy.Quest.prototype.start.call(this), "lamplight-B1" === i._zoneName && (Util.isDefined(this.zone.getQuestState()) ? this.cutScene(e, t, i) : (this.zone.startQuest(1), this.cutScene(e, t, i)))
	},
	cutScene: function (e, t, i) {
