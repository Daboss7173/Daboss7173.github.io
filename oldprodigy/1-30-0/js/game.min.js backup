}, Prodigy.extends(Prodigy.Menu.MenuBar, Prodigy.Control.Element, {
	constructor: Prodigy.Menu.MenuBar,
	addMenu: function (e, t) {
		Util.isDefined(this.bars[e]) || (this.bars[e] = this.game.prodigy.create.element(this, 0, 670)), this.bars[e].removeAll(!0);
		for (var i = this.game.prodigy.create.element(this.bars[e]), a = this.game.prodigy.create.element(this.bars[e]), s = 0; s < t.length; s++) {
			var r = this.game.prodigy.create.button(a, 90 * s, -40, "icons", t[s].icon, t[s].callback);
			t[s].jump && this.game.add.tween(r).to({
				y: r.y - 10
			}, 1e3, Phaser.Easing.Quadratic.InOut, !0, 0, Number.MAX_VALUE, !0)
		}
		var o = Math.ceil((a.width + 40) / 40),
			n = this.game.prodigy.create.panel(i, 0, -40, o, 2, "banner");
		a.x = Math.floor((n.width - a.width) / 2), this.centerBars()
	},
