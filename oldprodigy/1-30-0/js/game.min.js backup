		if (this.c.visible = !0, e) {
			this.c.removeAll(!0), this.e.removeAll(!0), this.title.setText("[membership] You are now a member! [membership]");
			var t = this.game.prodigy.create.panel(this.e, 580, 180, 8, 6, "panel-base");
