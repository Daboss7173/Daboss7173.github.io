		Util.isDefined(a) ? a < i.length && i[a].highlight(!0) : i[0].highlight(!0)
	},
	furnaceClick: function (e, t, i, a) {
		var s = this._zone.getState("furnace");
		s === i - 1 || Util.isDefined(s) || 1 === i || (s = 0);
