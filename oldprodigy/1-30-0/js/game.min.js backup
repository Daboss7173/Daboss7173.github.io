	presentClick: function (e, t) {
		if (this.getState(e, "1stpresent")) {
			var i = function (e) {
				if (this.getDate() !== this.getState(e, "event") && (this.presentactive = !0), this.presentactive) {
					var t = this.getDate();
					if (this.getState(e, "event") !== t) {
						var i = [{
								type: "item",
