	this.game = e
}, Prodigy.Attacks.prototype = {
	constructor: Prodigy.Attacks,
	calculateDamage: function (e, t, i, a) {
		Util.isDefined(a) || (a = 0);
		var s = 0;
		Util.isDefined(e) && Util.isDefined(e.damage) && (s = e.damage);
		var r = e.element,
			o = i.getElement();
		return "Glacial Shield" !== e.name && (this.isStrong(r, o) ? s += 4 : this.isWeak(r, o) && (s -= 3, 0 >= s && (s = 2))), Math.max(Math.floor(s + (Util.isDefined(a) ? a : 0)), 0)
	},
	isStrong: function (e, t) {
		return Util.isDefined(e) && Util.isDefined(t) ? "fire" !== e || "ice" !== t && "earth" !== t ? "water" === e && "fire" === t ? !0 : "earth" === e && "water" === t ? !0 : "storm" === e && "water" === t ? !0 : "mech" === e && "mech" === t ? !0 : "ice" !== e || "storm" !== t && "mech" !== t ? !1 : !0 : !0 : !1
	},
	isWeak: function (e, t) {
		return Util.isDefined(e) && Util.isDefined(t) ? "fire" === e && "water" === t ? !0 : "water" !== e || "earth" !== t && "storm" !== t && "mech" !== t ? "earth" === e && "fire" === t ? !0 : "storm" === e && "ice" === t ? !0 : "mech" === e && "ice" === t ? !0 : "ice" === e && "fire" === t ? !0 : !1 : !0 : !1
	},
	getBestAttack: function (e, t, i) {
		if (e.length <= 0) return null;
		for (var a = [], s = 0; s < e.length; s++) {
			var r = this.getAttack(e[s]);
			a[s] = "Glacial Shield" === r.name ? {
				dmg: 0,
				ID: e[s]
			} : {
				dmg: this.calculateDamage(r, null, t, 0),
				ID: e[s]
			}
		}
		a.sort(function (e, t) {
			return t.dmg - e.dmg
		}), i = i || 0;
		for (var o = a[a.length - 1].ID, s = 0; s < a.length; s++) {
			if (Math.random() > i) {
				o = a[s].ID;
				break
			}
			i *= .75
		}
		return this.getAttack(o)
	},
	getHealingAttack: function (e) {
		for (var t = 0; t < e.length; t++) {
			var i = this.getAttack(e[t]);
			if (i.heal) return i
		}
		return null
	},
	getAttack: function (e) {
		var t = Prodigy.Attacks.data[e - 1];
		return Util.isDefined(t) ? t : null
	}
