		for (var o = 500, n = 0; 20 > n; n++) 19 === n && (o = 500), r.to({
			x: 0,
			y: 0
		}, 10, Phaser.Easing.Linear.None), r.to({
			x: 19 === n ? 15 : 10,
			y: 19 === n ? 15 : 10
		}, o, Phaser.Easing.Linear.None), a.to({
			alpha: 1
		}, 10, Phaser.Easing.Linear.None), a.to({
			alpha: 0
		}, o, Phaser.Easing.Linear.None), t.to({
			x: 1.4,
			y: 1.4
		}, Math.floor(o / 2), Phaser.Easing.Quadratic.Out), t.to({
			x: 1,
			y: 1
		}, Math.floor(o / 2), Phaser.Easing.Quadratic.In), o -= 25;
		t.start(), a.start(), r.start()
	},
