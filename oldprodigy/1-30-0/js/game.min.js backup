				face: this.game.prodigy.create.sprite(350, 10, "boss-13", "expression-1"),
				strawberry: this.game.prodigy.create.sprite(400, -200, "boss-13", "berry")
			}, this.cakeParts.face.animations.add("attack", ["expression-1", "expression-2", "expression-3", "expression-3", "expression-3", "expression-2", "expression-1"], 10, !1, !1), this.cakeParts.lava.anchor.setTo(0, 1), this.cakeParts.face.anchor.setTo(0, 1);
			for (var i in this.potParts) this.add(this.potParts[i]);
			for (var i in this.burgerParts) this.add(this.burgerParts[i]), this.burgerParts[i].visible = !1;
			for (var i in this.cakeParts) this.add(this.cakeParts[i]), this.cakeParts[i].visible = !1;
			this.npc = new Prodigy.CebolliniEvent(this.game, this, -100, 0, "npc-sprite-cebollini", null), this.npc.flip(), this.npc.bossStart(this.game), this.complete = !0, this.loading = !1
		}
	},
	attack: function () {
		if ("soup" === this.foodMode) {
			var e = this.game.prodigy.event.create();
			e.animation(this.game.prodigy.animation.rotate.bind(this.game.prodigy.animation, this.potParts.lid, "35", 1e3, 0, null, !0)), e.animation(this.game.prodigy.animation.translate.bind(this.game.prodigy.animation, this.potParts.lid, "0", "-20", 1e3, 0, null, !0), !0), e.animation(this.game.prodigy.animation.rotate.bind(this.game.prodigy.animation, this.potParts.lid, "-35", 1e3, 0, null, !0)), e.animation(this.game.prodigy.animation.translate.bind(this.game.prodigy.animation, this.potParts.lid, "0", "20", 1e3, 0, null, !0));
			var t = this.game.prodigy.event.create();
			t.animation(this.game.prodigy.animation.rotate.bind(this.game.prodigy.animation, this.potParts.pot, "-10", 1e3, 0, null, !0), !0), t.animation(this.game.prodigy.animation.rotate.bind(this.game.prodigy.animation, this.potParts.pot, "10", 1e3, 0, null, !0)), e.start(), t.start()
		} else if ("burger" === this.foodMode) {
			var e = this.game.prodigy.event.create();
			e.animation(this.game.prodigy.animation.rotate.bind(this.game.prodigy.animation, this.burgerParts.top, "25", 700, 0, null, !0)), e.animation(this.game.prodigy.animation.translate.bind(this.game.prodigy.animation, this.burgerParts.top, "0", "-20", 700, 0, null, !0), !0), e.animation(this.game.prodigy.animation.rotate.bind(this.game.prodigy.animation, this.burgerParts.top, "-25", 700, 0, null, !0)), e.animation(this.game.prodigy.animation.translate.bind(this.game.prodigy.animation, this.burgerParts.top, "0", "20", 700, 0, null, !0));
			var t = this.game.prodigy.event.create();
			t.animation(this.game.prodigy.animation.rotate.bind(this.game.prodigy.animation, this.burgerParts.bottom, "-10", 700, 0, null, !0), !0), t.animation(this.game.prodigy.animation.rotate.bind(this.game.prodigy.animation, this.burgerParts.bottom, "10", 700, 0, null, !0)), e.start(), t.start()
		} else "cake" === this.foodMode && this.cakeParts.face.animations.play("attack")
	},
	startEntree: function () {
		this.foodMode = "burger", this.npc.entreeStart();
		var e = this.game.prodigy.event.create();
		e.text(11, "cebollini"), e.enableInput(!1);
