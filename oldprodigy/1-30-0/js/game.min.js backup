		switch (Prodigy.RenderMenu.prototype.setMode.call(this, e), this.clearContents(), e) {
		case 1:
			this.itemType = "outfit";
			break;
		case 2:
			this.itemType = "weapon";
			break;
		case 3:
			this.itemType = "boots";
			break;
		case 4:
			this.itemType = "relic";
			break;
		case 5:
			this.itemType = "item";
			break;
		default:
			this.itemType = "hat"
		}
		this.listItemsOfType()
	},
