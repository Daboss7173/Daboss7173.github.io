}, Prodigy.ChatManager = function () {}, Prodigy.ChatManager.prototype = {
	getEmotes: function () {
		return Prodigy.ChatManager.EMOTES
	},
	getEmote: function (e) {
		var t = this.getEmotes();
		return t[e]
	},
	getPhrase: function (e) {
		var t = this.getPhrases();
		return console.log(t.length), t[e]
	},
	getPhraseID: function (e) {
		for (var t = this.getPhrases(), i = 0; i < t.length; i++)
			if (t[i] === e) return i;
		return 0
	},
	getPhrases: function () {
		if (Util.isDefined(this._phrases)) return this._phrases;
		for (var e = [], t = 0; t < Prodigy.ChatManager.CHAT.length; t++) e.push(Prodigy.ChatManager.CHAT[t]);
		for (var i in Monsters.data) e.push(Monsters.data[i].name);
		for (var i in Items.data)
			for (var a = Items.data[i], t = 0; t < a.length; t++) e.push(a[t].name);
		this._phrases = e;
		for (var t = 0; t < this._phrases.length; t++) this._phrases[t] = Util.capitalize(this._phrases[t]);
		var s = {};
		return this._phrases = e.filter(function (e) {
			return s.hasOwnProperty(e) ? !1 : s[e] = !0
		}), this._phrases
	}
