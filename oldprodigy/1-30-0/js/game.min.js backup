		for (var t in this.cakeParts) e.set(this.cakeParts[t], "visible", !0);
		for (var t in this.cakeParts) e.animation(this.game.prodigy.animation.hop.bind(this.game.prodigy.animation, this.cakeParts[t], "-400", "-150", "+150", 2e3, !1, !0));
		e.delay(2e3), e.enableInput(!0), e.start()
	}
