		bonfire_spire: new Prodigy.Bonfire_Spire(e)
	}, this.states = ["bounties"]
}, Prodigy.World.prototype = {
	teleport: function (e, t, i, a) {
		this.process(), this._currentZone = e;
		var s = e.split("-");
		if (Util.isDefined(this.zones[s[0]])) this.zones[s[0]].teleport(s[1], t, i, a);
		else switch (e) {
		case "dino-1":
			this.game.state.start("Museum");
			break;
		case "house-0":
			this.game.state.start("Dorm");
			break;
		case "pirate-0":
			this.game.state.start("Pirate");
			break;
		case "tower-0":
			this.game.state.start("TowerBase");
			break;
		case "town-2":
			this.game.state.start("Arena");
			break;
		case "tech-0":
			this.game.state.start("Tech");
			break;
		default:
			this.zones.lamplight.teleport("B1", t, i, a)
		}
	},
	getCurrentMap: function () {
		return this._currentZone
	},
	getState: function (e) {
		var t = e;
		return this.game.prodigy.player.state.get("world-" + t)
	},
	setState: function (e, t) {
		var i = e;
		this.game.prodigy.player.state.set("world-" + i, t)
	},
	getZone: function (e) {
		return this.zones[e]
	},
	process: function () {
		this.getDate() !== this.getTownState("bounty") && (this.game.prodigy.bounty.generateBounties(this.getDate()), this.setTownState(this.getDate(), "bounty"))
	},
	getTownState: function (e) {
		var t = this.getZone("lamplight");
		return t.getState(e)
	},
	setTownState: function (e, t) {
		var i = this.getZone("lamplight");
		i.setState(t, e)
	},
	getDate: function () {
		var e = new Date,
			t = e.getDate(),
			i = e.getMonth() + 1,
			a = e.getFullYear();
		return 10 > t && (t = "0" + t), 10 > i && (i = "0" + i), i + "" + t + a
	}
