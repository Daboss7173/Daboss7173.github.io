			else if ("win" === e) {
				for (var i = this.teams[1].getDefeated(), a = 0; a < i.length; a++) Util.isDefined(this.mods.quest) && this.game.prodigy.player.quests.defeatMonster(this.mods.zoneID, this.mods.quest, i[a].getID());
