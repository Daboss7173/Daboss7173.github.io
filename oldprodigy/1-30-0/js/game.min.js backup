	Prodigy.Zone.call(this, e), this.name = "Lamplight", this.ID = "lamplight", this.game = e, this.battleBG = "bg-battle-arena", this.onFaint = "lamplight-B1", this.questHub = "lamplight-B1", this.states = ["event", "1stpresent", "bounty"], this.util = new Prodigy.LamplightUtil(e);
