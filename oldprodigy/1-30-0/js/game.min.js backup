			var t = 3 > e ? 3 : 5 > e ? 4 : 5;
			this.poses = [];
			for (var i = 0; t > i; i++) this.poses[i] = new DancePose(this.game, this.poseElement, 130 * i, 0, this.selectAnswer.bind(this, i));
