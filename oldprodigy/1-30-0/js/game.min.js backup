		this.scrollB = this.game.prodigy.create.slider(this, e + 720 - 50, t, 380, !0, !0)
	},
	showAchievements: function (e, t) {
		for (var i = 0; i < this.achievements.length; i++) this.achievements[i].setSource(e[t + i])
	}
