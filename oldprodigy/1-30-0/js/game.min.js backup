		this[i] = this.game.prodigy.math.clamp(this[i] + a, 0, t.length - 1);
		for (var s = 0; 3 > s; s++) {
			var r = t[this[i] - 1 + s];
			e[s].setText(Util.isDefined(r) ? r : "")
		}
	},
